//Practical No. 3; conversion from F to C
//Date: 20/09/2022
//Author: Firoz dabir
#include <stdio.h>

int main() {
    float fahrenheit;
    float celsius;
    printf("Enter temperature in fahrenheit: ");
    scanf("%f",&fahrenheit);
    celsius=(fahrenheit-32)*5/9;
    printf("Temperature in celsius = %f",celsius);

    return 0;
}
