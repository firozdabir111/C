//Practical No.1: Whether the number is even or odd
#include <stdio.h>

int main() {
    int a;
    printf("Enter the number\n");
    scanf("%d",&a);
    if(a%2==0){
        printf("The number is even");
    }
    else{
        printf("The number is odd");
    }
    return 0;
}
