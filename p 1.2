//Practical No.1: Swapping of two numbers
#include <stdio.h>

int main() {
    int a;
    int b;
    int c;
    printf("Enter first variable, a: \n");
    scanf("%d",&a);
    printf("Enter second variable, b: \n");
    scanf("%d",&b);
    c=a;
    a=b;
    b=c;
    printf("Value of a = %d \n",a);
    printf("Value of b = %d \n",b);

    return 0;
}
