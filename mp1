// addition of 3x3 2d matrix
#include <stdio.h>

int main() {
    int a[3][3],b[3][3],c[3][3],i,j;
    printf("First Matrix;\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("Enter R%d and C%d:",i,j);
            scanf("%d",&a[i][j]);
        }
    }
    printf("Second Matrix:\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("Enter R%d and C%d:",i,j);
            scanf("%d",&b[i][j]);
        }
    }
    printf("Sum of two Matrix:\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            c[i][j]=a[i][j]  + b[i][j];
            printf("%d\t",c[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}
